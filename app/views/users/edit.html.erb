<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

<h1>User info</h1>
<%= form_with model: @user do |f| %>
  <h4>Name<h4>
  <%= f.text_field :name %>
  <h4>Image</h4>
  <%= f.file_field :hoge %>
  <h4>Introduction</h4>
  <%= f.text_area :body %><br>
  <%= f.submit "Update User" %>
<% end %>
